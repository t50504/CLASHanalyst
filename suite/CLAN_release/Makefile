all:
	$(MAKE) -C src all
	mv src/clan_index src/clan_search src/clan_output src/clan_annotate bin/
clan_index:
	$(MAKE) -C src clan_index
	mv src/clan_index bin/
clan_search:
	$(MAKE) -C src clan_search
	mv src/clan_search bin/
clan_output:
	$(MAKE) -C src clan_output
	mv src/clan_output bin/
clan_annotate:
	$(MAKE) -C src clan_annotate
	mv src/clan_annotate bin/
clean:
	$(MAKE) -C src clean
new:
	$(MAKE) -C src new
